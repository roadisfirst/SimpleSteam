{
  "name": "simple-steam-node",
  "version": "0.0.0",
  "engines": {
    "node": "~14.17.1",
    "npm": "~7.19.1"
  },
  "description": "node server",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js", 
    "client-install": "npm install --prefix client", 
    "client": "npm run start --prefix client", 
    "dev": "concurrently \"npm run server\" \"npm run client\"", 
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run client-install && npm run build --prefix client" 
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roadisfirst/SimpleSteam.git"
  },
  "author": "Tetiana Filatova",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/roadisfirst/SimpleSteam/issues"
  },
  "homepage": "https://github.com/roadisfirst/SimpleSteam#readme",
  "dependencies": {
    "express": "^4.17.1",
    "@heroku-cli/plugin-buildpacks": "^7.54.0",
    "@heroku/buildpack-registry": "^1.0.1",
    "async-file": "^2.0.2",
    "heroku": "^7.56.1",
    "heroku-cli": "^7.0.9",
    "phoenix": "^1.5.10",
    "valid-url": "^1.0.9",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.7",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12"
  }
}
